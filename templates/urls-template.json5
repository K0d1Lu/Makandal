  "board edit": "/dam/wedia/boards/{{BOARD_ID}}/edit",
  "public boards": "/dam/wedia/boards?visibility=public",
  
  // ============ PORTALS ============
  "portals page": "/dam/wedia/portals/",
  "portal detail": "/dam/wedia/portals/{{PORTAL_ID}}/",
  "portal edit": "/dam/wedia/portals/{{PORTAL_ID}}/edit",
