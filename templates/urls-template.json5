{
  // Template de définitions d'URLs pour les tests Cucumber
  // Organisé par sections fonctionnelles
  
  // ============ PAGES PRINCIPALES ============
  "home page": "/dam/wedia/home",
  "dam page": "/dam/wedia/browse",
  "search page": "/dam/wedia/preferences/searches",
  "notifications page": "/dam/wedia/usernotif",
  "profile page": "/dam/wedia/profile",
  
  // ============ AUTHENTIFICATION ============
  "signin page": "/dam/wedia/signin",
