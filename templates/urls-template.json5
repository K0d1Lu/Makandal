  "upload page": "/dam/wedia/upload",
  "import page": "/dam/wedia/dam-import",
  "bulk upload": "/dam/wedia/upload/bulk",
  
  // ============ ADMIN ============
  "admin page": "/dam/wedia/admin",
  "users management": "/dam/wedia/admin/users",
  "permissions": "/dam/wedia/admin/permissions",
  "system settings": "/dam/wedia/admin/settings",
  
  // ============ API ENDPOINTS ============
  "api assets": "/api/wedia/assets",
  "api boards": "/api/wedia/boards", 
  "api upload": "/api/wedia/upload",
  "api search": "/api/wedia/search",
  
  // ============ EXTERNAL LINKS ============
  "help documentation": "https://help.wedia.com",
  "support contact": "https://support.wedia.com",
  "wedia website": "https://www.wedia.com",
  
  // ============ QUERY PARAMETERS ============
  // Exemples avec paramètres de requête
  "filtered assets": "/dam/wedia/browse?filter={{FILTER_TYPE}}",
  "paginated results": "/dam/wedia/browse?page={{PAGE_NUMBER}}",
  "sorted results": "/dam/wedia/browse?sort={{SORT_FIELD}}&order={{SORT_ORDER}}",
  
  // ============ DEEP LINKS ============
  // URLs avec ancres ou fragments
  "asset metadata tab": "/dam/wedia/browse/all-assets/asset/{{ASSET_ID}}/#metadata",
  "asset versions tab": "/dam/wedia/browse/all-assets/asset/{{ASSET_ID}}/#versions",
  "board settings": "/dam/wedia/boards/{{BOARD_ID}}/#settings",
  
  // ============ CUSTOM URLS ============
  // Ajoutez ici vos URLs spécifiques au projet
  
  // Convention de nommage recommandée :
  // "[section] [action/type] [modifier]" : "url_pattern"
  // Exemples :
  // "dam search results": "/dam/wedia/browse?q={{SEARCH_TERM}}"
